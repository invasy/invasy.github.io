{
  "id": "hugo-env",
  "version": "0.1.0",
  "name": "Hugo Environment",
  "description": "Setup Hugo environment",
  "installsAfter": ["./user", "./golang", "./hugo"],
  "containerEnv": {
    "GOROOT": "/opt/go",
    "GOPATH": "/home/hugo/go",
    "GOBIN": "/home/hugo/go/bin",
    "PATH": "/home/hugo/go/bin:/opt/go/bin:$PATH"
  }
}