{
  "id": "hugo",
  "version": "0.1.0",
  "name": "Hugo",
  "description": "Install Hugo",
  "installsAfter": ["./packages", "./user", "./golang"],
  "options": {
    "variant": {
      "description": "Hugo variant",
      "type": "string",
      "enum": ["hugo", "hugo_extended"],
      "default": "hugo_extended"
    },
    "version": {
      "description": "Hugo version",
      "type": "string",
      "default": "latest"
    },
    "sass_version": {
      "description": "SASS version",
      "type": "string",
      "default": "latest"
    },
    "pagefind_version": {
      "description": "PageFind version",
      "type": "string",
      "default": "latest"
    }
  }
}